        </activity>
        -->
        <activity android:name=".CreateMemesActivity">
        </activity>
        <activity
            android:name=".FullImageActivity"
            android:label="Image"
            android:theme="@style/AppTheme.Translucent">
        </activity>
    </application>

</manifest>